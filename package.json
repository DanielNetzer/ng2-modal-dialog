{
  "name": "ng2-modal-dialog",
  "version": "2.0.1",
  "description": "AngularX Factory made Modal window",
  "main": "modal.module.ts",
  "repository": "https://github.com/DanielNetzer/ng2-modal-dialog",
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/ngmodaldialog.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ngmodaldialog.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify"
  },
  "dependencies": {
    "@angular/core": "^4.3.6",
    "rxjs": "^5.4.3"
  },
  "keywords": [
    "angular2",
    "ng2",
    "modal",
    "dialog",
    "Factory"
  ],
  "author": "Daniel Netzer <daniel.netzer7@gmail.com>",
  "contributors": [
    "Ron Netzer <ron@peak.studio>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@angular/compiler": "4.4.3",
    "@angular/compiler-cli": "4.4.3",
    "rollup": "0.50.0",
    "typescript": "2.5.3",
    "uglify-js": "3.1.2"
  }
}