    "angular2",
    "modal",
    "dialog",
    "Factory"
  ],
  "author": "Daniel Netzer <daniel.netzer7@gmail.com>",
  "contributors": [
    "Ron Netzer <ron@peak.studio>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@angular/compiler": "5.0.1",
    "@angular/compiler-cli": "5.0.1",
    "rollup": "0.51.3",
    "typescript": "2.6.1",
    "uglify-js": "3.1.8"
  }
}